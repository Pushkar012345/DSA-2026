/**
 * @param {number[]} nums
 * @param {number} k
 * @return {number}
 */
var longestOnes = function(nums, k) {
    let i=0;

    for(let j=0;j<nums.length;j++)
    {
        if(nums[j]===0)
        {
            k--;
        }

        if(k<0)
        {
            if(nums[i]===0)
            {
                k++;
            }
            i++;
        }


    }

    return nums.length-i;


};
